#include <string.h>

using namespace std;

int main() 

{

   float arr_f[50];
int i;
for(i=0; i<40; i++)
 {
 	fprintf(stdout,"Enter a Number:");
 	fscanf(stdin,"%f",&arr_f[i]);
	
	
}

	return 0;
}



