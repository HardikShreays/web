/*Switch*/
#include <iostream>
using namespace std;
int main()
{
	int age;
	cout<<"What's your age?";
	cin>>age;
	switch(age)
	{
		case 1: cout<<"You're 1 year old.";
		break;
		case 2: cout<<"You're 2 year old.";
		break;
		case 3: cout<<"You're 3 year old.";
		break;
		default: cout<<"You're older than 3.";
	}
	
return 0;
}
