#include <stdio.h>

int main() {
  int age;
  
  fprintf(stdout, "Enter age: ");
  fscanf(stdin, "%d", &age);
  
  if ( age >= 0 && age <= 3) {
    fprintf(stdout, "Baby\n");
  } else if (age >3 && age<=7) {fprintf(stdout,"Kindergarden\n");
  }
else if (age >7 && age<=18) {fprintf(stdout,"School\n");}
else if (age >18 && age<=25) {fprintf(stdout,"uni\n");}
else if (age >25) {fprintf(stdout,"work\n");
  }   
    return 0;
}
